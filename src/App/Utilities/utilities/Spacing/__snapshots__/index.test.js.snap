exports[`Utilities: Spacing HowItWorks renders 1`] = `
<Fragment>
  <h2
    id="how-it-works"
  >
    How it works
  </h2>
  <p>
    Assign responsive-friendly 
    <CodeTags
      code="margin"
      type="secondary"
    />
     or 
    <CodeTags
      code="padding"
      type="secondary"
    />
     values to an element or a subset of its sides with shorthand classes. Includes support for individual properties, all properties, and vertical and horizontal properties. Classes are ranging from 
    <CodeTags
      code="0.25rem"
      type="secondary"
    />
     to 
    <CodeTags
      code="3rem"
      type="secondary"
    />
    .
  </p>
</Fragment>
`;

