exports[`Components: Topbar Overview renders 1`] = `
<Fragment>
  <h2
    id="overview"
  >
    Overview
  </h2>
  <p>
    To use a topbar the bare minimum markup required is:
  </p>
  <ul>
    <li>
      <CodeTags
        code=".topbar"
        type="secondary"
      />
       wrapper.
    </li>
    <li>
      Anchor with class 
      <CodeTags
        code=".topbar-logo"
        type="secondary"
      />
       to display the logo and make it clickable.
    </li>
  </ul>
  <p>
    With this you get a topbar with the Swedbank Pay logo. In the topbar you can add additional functionality like a menu containing links. To enable this add a 
    <CodeTags
      code="<button>"
      type="primary"
    />
     and a 
    <CodeTags
      code="<nav>"
      type="primary"
    />
     containing anchors. Remember to include 
    <CodeTags
      code="toggle-nav=\\"{your_nav_id}\\""
      type="secondary"
    />
     as a 
    <CodeTags
      code="<button>"
      type="primary"
    />
     attribute to get the menu to appear when the menu-button is clicked.
  </p>
  <ComponentPreview
    codeFigure={true}
    language="html"
    showCasePanel={true}
  >
    <Topbar
      id="demo-topbar"
      logout={true}
      topbarContent={
        Object {
          "btn": Object {
            "icon": "menu",
            "name": "Menu",
          },
          "items": Array [
            Object {
              "icon": "home",
              "name": "Home",
            },
            Object {
              "icon": "shopping_cart",
              "name": "Purchase history",
            },
            Object {
              "icon": "settings",
              "name": "Settings",
            },
          ],
        }
      }
    />
  </ComponentPreview>
</Fragment>
`;

